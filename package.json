{
  "name": "recallio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "dev": "next dev",
    "build": "npm run db-migrate&& next build",
    "analyze": "set ANALYZE=true&& next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check-format": "prettier . -c",
    "format": "pnpm check-format -w",
    "db-migrate": "drizzle-kit generate:mysql && tsx ./scripts/database/migration.ts",
    "db-studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@auth/core": "^0.12.0",
    "@planetscale/database": "^1.11.0",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.33.0",
    "@trpc/client": "^10.38.0",
    "@trpc/next": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "drizzle-orm": "^0.28.5",
    "html5-qrcode": "^2.3.8",
    "next": "^13.4.19",
    "next-auth": "^4.23.1",
    "node-html-parser": "^6.1.6",
    "postcss": "^8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.3",
    "superjson": "^1.13.1",
    "tailwindcss": "^3.3.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "msw": "^1.2.3",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.18.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "next-auth@4.23.1": "patches/next-auth@4.23.1.patch"
    }
