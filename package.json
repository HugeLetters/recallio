  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "pnpm cleanup&& next dev -p 1853",
    "build": "pnpm db-migrate&& pnpm nextjs-routes&& next build",
    "analyze": "ANALYZE=true next build",
    "cleanup": "rm -rf .next",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check-format": "prettier . -c",
    "format": "pnpm check-format -w",
    "copypaste-audit": "jscpd -i \"**/node_modules/**/*,**/database/migrations/**/*,**/.next/**/*\" -p \"**/src/**/*\"",
    "rscript": "tsx ./scripts/run.ts",
    "db-studio": "drizzle-kit studio",
    "db-seed": "pnpm rscript database/seed",
    "db-migrate": "drizzle-kit generate:mysql && pnpm rscript database/migration"
  },
